SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=<REDACTED>
OPENAI_API_KEY=<REDACTED>

# Redis (optional): prefer REDIS_URL; for TLS use rediss:// and the TLS port
# Examples:
# REDIS_URL=redis://default:<REDACTED>@host:port/0
# REDIS_URL=rediss://default:<REDACTED>@host:tls_port/0
REDIS_URL=
REDIS_NAMESPACE=agentic-dev

# Persistence backend kind: supabase | memory
PERSIST_KIND=memory

# Test gating for live integrations
USE_REAL_TESTS=0